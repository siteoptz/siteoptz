# NextAuth.js Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-secret-key-here-change-in-production

# Google OAuth Configuration for Unified Services Integration
# Get these from: https://console.cloud.google.com/
# IMPORTANT: Configure OAuth consent screen with these scopes:
# - Google Ads: https://www.googleapis.com/auth/adwords
# - Search Console: https://www.googleapis.com/auth/webmasters.readonly  
# - Tag Manager: https://www.googleapis.com/auth/tagmanager.readonly
# - Analytics: https://www.googleapis.com/auth/analytics.readonly
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# GoHighLevel CRM Integration
# Get these from your GoHighLevel account
GOHIGHLEVEL_API_KEY=your-gohighlevel-api-key
GOHIGHLEVEL_LOCATION_ID=your-location-id
ENABLE_GHL=true

# Optional: GoHighLevel Pipeline Configuration for Free Plan
GHL_FREE_PLAN_PIPELINE_ID=your-free-plan-pipeline-id
GHL_FREE_PLAN_STAGE_ID=your-free-plan-stage-id

# Legacy GoHighLevel Environment Variables (for backwards compatibility)
GHL_API_KEY=your-gohighlevel-api-key
GHL_LOCATION_ID=your-location-id
GHL_PIPELINE_ID=your-pipeline-id
GHL_PIPELINE_STAGE_ID=your-pipeline-stage-id

# Copy this file to .env.local and fill in your actual values
# Note: .env.local is automatically ignored by git for security