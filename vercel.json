{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "https://app.cyfe.com/:path*",
      "has": [
        {
          "type": "host",
          "value": "optz.siteoptz.ai"
        }
      ]
    }
  ]
}
